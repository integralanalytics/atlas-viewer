# Atlas Viewer Environment Variables
# Copy this file to .env.local and fill in your actual values

# MapBox API Token (required for MapBox base maps)
# Get your token from: https://account.mapbox.com/access-tokens/
REACT_APP_MAPBOX_TOKEN=your_mapbox_token_here

# Optional: Alternative map service tokens
# REACT_APP_MAPTILER_TOKEN=your_maptiler_token
# REACT_APP_GOOGLE_MAPS_TOKEN=your_google_maps_token

# Development settings
REACT_APP_ENV=development
REACT_APP_DEBUG=true

# API endpoints (if you have a backend)
# REACT_APP_API_BASE_URL=https://your-api.example.com

# Feature flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_CLOUD_STORAGE=false
